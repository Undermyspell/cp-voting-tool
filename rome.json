{
    "linter": {
      "enabled": true,
      "rules": {
        "recommended": true,
        "suspicious": {
          "noExplicitAny": "off",
          "noAsyncPromiseExecutor": "off"
        }
      },
      "ignore": []
    },
    "formatter": {
      "enabled": true,
      "formatWithErrors": false,
      "indentStyle": "tab",
      "indentSize": 2,
      "lineWidth": 150,
      "ignore": []
    },
    "javascript": {
      "formatter": {
        "quoteStyle": "double",
        "trailingComma": "none",
        "semicolons": "asNeeded"
      }
    },
    "files": {
      "ignore": []
    }
  }
  